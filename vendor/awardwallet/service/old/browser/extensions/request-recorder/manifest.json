{
    "version": "1.0.0",
    "key": "",
    "manifest_version": 2,
    "name": "Request Recorder",
    "applications": {
        "gecko": {
            "id": "request-recorder@awardwallet.com"
        }
    },
    "permissions": [
        "browsingData",
        "tabs",
        "unlimitedStorage",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "minimum_chrome_version":"22.0.0",
    "externally_connectable": {
        "matches": ["http://localhost/*"],
        "ids": ["*"]
    }
}