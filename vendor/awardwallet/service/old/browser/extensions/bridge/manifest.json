{
    "manifest_version": 2,
    "name": "extension bridge, paired with ExtensionCommunicator",
    "version": "1.0.0",
    "key": "",
    "applications": {
        "gecko": {
            "id": "bridge@awardwallet.com"
        }
    },
    "externally_connectable": {
        "matches": ["*://awardwallet-browser-control.s3.amazonaws.com/*"],
        "ids": ["*"]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "browsingData",
        "cookies",
        "proxy",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle",
            "all_frames": false
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "externally_connectable": {
        "matches": ["http://localhost/*"]
    }
}